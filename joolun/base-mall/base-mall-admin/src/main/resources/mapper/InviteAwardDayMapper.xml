<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.joolun.cloud.mall.admin.mapper.InviteAwardDayMapper">

    <resultMap id="inviteAwardDayMap" type="com.joolun.cloud.mall.common.entity.InviteAwardDay">
        <id property="id" column="id"/>
        <result property="inviterUserMealId" column="inviter_user_meal_id"/>
        <result property="inviteeUserMealId" column="invitee_user_meal_id"/>
        <result property="endTime" column="end_time"/>
        <result property="inviterUserId" column="inviter_user_id"/>
    </resultMap>

    <sql id="inviteAwardDaySql">
        invite_award_day.`id`,
        invite_award_day.`inviter_user_meal_id`,
        invite_award_day.`invitee_user_meal_id`,
        invite_award_day.`end_time`,
        invite_award_day.`inviter_user_id`
    </sql>
</mapper>
